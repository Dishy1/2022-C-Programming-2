// 이 부분에 필요한 코드를 작성하시오.

int add(int a, int b){
  return a+b;
}

// ===============================================

int main() {

    int a, b;
    scanf("%d %d", &a, &b);

    printf("%d", add(a, b));

    return 0;
}

